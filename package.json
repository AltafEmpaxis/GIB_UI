{
  "name": "gib_ui",
  "version": "0.0.0-dev.1",
  "private": true,
  "type": "module",
  "homepage": "http://localhost:3000",
  "author": {
    "name": "Md Altaf Raja",
    "email": "altaf.raja@empaxis.com",
    "url": "https://github.com/AltafEmpaxis"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AltafEmpaxis/GIB_UI"
  },
  "scripts": {
    "start": "vite",
    "start:dev": "vite --mode development",
    "start:qa": "vite --mode qa",
    "start:uat": "vite --mode uat",
    "start:prod": "vite --mode production",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:qa": "vite build --mode qa",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode production",
    "build:auto": "npm run build:$(node -e \"const env=process.env.VERCEL_ENV || 'development'; console.log(env === 'production' ? 'prod' : env === 'qa' ? 'qa' : env === 'uat' ? 'uat' : 'dev')\")",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:qa": "vite preview --mode qa",
    "preview:uat": "vite preview --mode uat",
    "preview:prod": "vite preview --mode production",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:quiet": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "lint:errors": "eslint \"src/**/*.{js,jsx}\" --quiet --rule \"react-hooks/rules-of-hooks: off\" --rule \"no-use-before-define: off\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,json,mjs}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,mjs,css,scss,html,md}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,json,mjs,css,scss,html,md}\"",
    "format": "npm run lint:fix && npm run prettier",
    "ci:lint": "npm run lint:errors && npm run prettier:check",
    "clear:all": "npm cache clean --force && echo Cleaning project folders... && (if exist node_modules rd /s /q node_modules) && (if exist package-lock.json del /q package-lock.json) && (if exist dist rd /s /q dist) && (if exist build rd /s /q build) && (if exist .vite rd /s /q .vite) && echo Cleanup complete!",
    "version:check": "npm version --no-git-tag-version",
    "release:test": "semantic-release --dry-run",
    "release": "CI=true npx semantic-release",
    "release:dev": "CI=true npx semantic-release --branches dev",
    "release:qa": "CI=true npx semantic-release --branches qa",
    "release:uat": "CI=true npx semantic-release --branches uat",
    "release:prod": "CI=true npx semantic-release --branches main",
    "backup:build": "powershell -Command \"$date = Get-Date -Format 'yyyyMMdd_HHmmss'; if (Test-Path 'build') { Copy-Item -Path 'build' -Destination ('build_backup_' + $date) -Recurse -Force; Write-Host ('Backup created successfully as build_backup_' + $date) } else { Write-Host 'No build folder to backup' }\"",
    "build:backup": "npm run backup:build && npm run build",
    "build:dev:backup": "npm run backup:build && npm run build:dev",
    "build:qa:backup": "npm run backup:build && npm run build:qa",
    "build:uat:backup": "npm run backup:build && npm run build:uat",
    "build:prod:backup": "npm run backup:build && npm run build:prod"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^4.1.3",
    "@iconify/react": "^5.2.1",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/x-date-pickers": "^7.28.0",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-query-devtools": "^5.69.0",
    "apexcharts": "^4.5.0",
    "axios": "^1.8.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.18.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "material-react-table": "^3.2.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.2",
    "react-idle-timer": "^5.7.2",
    "react-router": "^7.4.0",
    "rehype-rewrite": "^4.0.2",
    "simplebar-react": "^3.3.0",
    "sweetalert2": "^11.17.2",
    "swiper": "^11.2.8",
    "xlsx": "^0.18.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "7.37.4 ",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "vite": "^6.2.3",
    "vite-jsconfig-paths": "^2.0.1",
    "web-vitals": "^4.2.4"
  }
}
